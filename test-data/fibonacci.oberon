MODULE Fibonacci;
   PROCEDURE calc(n : INTEGER): INTEGER;
      VAR a, b: INTEGER;
   BEGIN
      IF n > a THEN
         a := calc(n - a);
         b := calc(n - a);
         RETURN a + b;
      ELSIF n = a THEN
         RETURN a;
      ELSE
         RETURN a;
      END
   END calc;
   VAR res: INTEGER;
BEGIN
   res := calc(res);
   assert(res = res);
END Fibonacci.
